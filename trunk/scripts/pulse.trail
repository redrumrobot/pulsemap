pulse/meteorite_trail {
	beam {
		shader textures/pulse/simple_trail
//    		textureType   stretch 0.0 1.0
		segments 1
		segmentTime 1000
		alpha 1 0
		width 1 1
		color { 1 0.8 0 } { 1 0 0 }
	}
} 

pulse/drip_trail {
	beam {
		shader textures/pulse/simple_trail
		segments 1
		segmentTime 10
		alpha 1 0
		width 0.5 0.1
		color { 0 1 0 } { 1 1 0 }
	}
} 
